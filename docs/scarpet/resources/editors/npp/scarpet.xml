                __on_player_takes_damage
                __on_player_trades
                __on_player_uses_item
                __on_player_wakes_up
                __on_player_escapes_sleep
                __on_statistic
                __on_start
                __on_tick
                __on_tick_ender
                __on_tick_nether
                __on_player_takes_damage
